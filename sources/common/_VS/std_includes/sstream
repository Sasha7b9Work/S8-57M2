#pragma once


namespace std
{
    struct asdf
    {
        char *c_str();
    };

    struct stringstream
    {
        asdf &str();
    };

    struct ostream
    {
    };

    struct fixed
    {
    };

    stringstream &operator<<(stringstream &, const fixed &);

    struct setprecision
    {
        setprecision(int) {};
    };
}
